/* pages/stuMsg5/stuMsg5.wxss */
/* pages/stuMsg4/stuMsg4.wxss */
.viewWrap {
  min-height: 100vh;
  border: 0px solid red;
}

.divWrap {
  display: block;
  height: 100%;
  border: 0px solid red;
  position: relative;
}

.titWrap {
  display: block;
  width: 90%;
  color: #16247B;
  font-size: 14px;
  margin: 5px auto;
  border: 0px solid red;
}
.fontSty {
  font-size: 18px;
  color: #171717;
}
.iconR {
  display: block;
  width: 26px;
  height: 26px;
  border: 0px solid red;
  float: right;
}
.iconR image {
  display: block;
  width: 100%;
  height: 100%;
}
.listWrap {
  display: block;
  width: 90%;
  margin: 5px auto;
  border: 0px solid red;
}
.once {
  display: block;
  width: 100%;
  height: 50px;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: flex-start;
  justify-content:  flex-start;
  margin: 5px auto;
}
.onceIn {
  display: inline-block;
  width: 30%;
  height: 100%;
  border: 0px solid red;
}
.t {
  display: inline-block;
  width: 100%;
  height: 25px;
  text-align: center;
}
.b {
  display: inline-block;
  width: 100%;
  height: 25px;
  white-space: nowrap;
  overflow: auto;
  text-align: center;
}
.minutesSty {
  display: inline-block;
  width: 26px;
  height: 26px;
  border: 0px solid red;
  margin-top: 12px;
}
.minutesSty image {
  display: block;
  width: 100%;
  height: 100%;
}

.weui-demo-dialog{
  visibility:hidden;
  opacity:0;
  transition:opacity .3s;
}
.weui-demo-dialog_show{
  visibility:visible;
  opacity:1;
}
.divForm {
  display: block;
  width: 100%;

  border: 0px solid red;
}
.inputOnce {
  display: block;
  width: 100%;
  height: 30px;
  margin: 5px auto;
  text-align: left;
  border: 0px solid red;
  line-height: 30px;
  color: #2C2C2C;
}
.inputOnce input{
  display: inline-block;
  max-width: 65%;
  height: 100%;
  float: right;
  color: #C4C4C4;
  font-size: 14px;
}

.listDialog {
  display: inline-block;
  width: 95%;
  margin: 0 auto;
  border: 0px solid red;
  height: 200px;
  overflow: auto;
}
.myRadioStyle {
  border: 0px solid red;
  display: block;
  height: 40px;
  line-height: 40px;
  margin-bottom: 2px;
  border-bottom: 1px solid #eee;
}
.tips {
  display: block;
  width: 90%;
  color: #A2A2A2;
  font-size: 14px;
  margin: 0 auto;
}