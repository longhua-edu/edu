/* pages/seledSta/seledSta.wxss */
.viewWrap {
  min-height: 100vh;
  border: 0px solid red;
}
.divWrap {
  display: block;
  height: 100%;
  border: 0px solid red;
  position: relative;
}
.seledWrap {
  display: block;
  width: 92%;
  margin: 0 auto;
}
.seledOnce {
  display: block;
  width: 100%;
  height: 100px;
  border: 1px solid #EBEBEB;
  border-radius: 6px;
  margin-bottom: 10px;
  background-color: #fff;
  box-shadow: 3px 3px 2px #004898;
}
.seledCenter {
  display: block;
  width: 95%;
  height: 80px;
  margin: 10px auto;
  border: 0px solid red;
}
.one {
  display: block;
  width: 100%;
  height: 25px;
  line-height: 25px;
}
.oneL {
  display: inline-block;
  height: 100%;
  float: left;
  max-width: 20%;
  white-space: nowrap;
  overflow: auto;
}
.oneR {
  display: inline-block;
  height: 100%;
  max-width: 80%;
  float: left;
  white-space: nowrap;
  overflow: auto;
}
.two {
  display: block;
  width: 100%;
  height: 30px;
  line-height: 30px;
}
.twoL {
  display: inline-block;
  height: 100%;
  float: left;
}
.green {
  color: #24A234;
}
.yellow {
  color: #F39903;
}
.red {
  color: #F52020;
}
.twoR {
  display: inline-block;
  height: 100%;
  float: right;
  border: 0px solid red;
  width: 30px;
  margin-right: 10px;
}
.twoR image {
  display: block;
  width: 100%;
  height: 100%;
}
.leftWrap {
  display: inline-block;
  box-sizing: border-box;
  height: 60px;
  width: 75%;
  border-radius: 5px;
  margin-top: 10px;
  border: 0px solid rebeccapurple;
  float: left;
  padding: 5px 10px;
  background-color: #fff;
}
.leftT {
  display: block;
  width: 100%;
  height: 50%;
  box-sizing: border-box;
  border: 0px solid red;
  border-bottom: 1px solid #ddd;
  color: #333;
  white-space: nowrap;
  overflow: auto;
}
.leftB {
  display: block;
  width: 100%;
  height: 50%;
  box-sizing: border-box;
  border: 0px solid red;
  font-size: 14px;
  color: #666;
}
.rightWrap {
  display: inline-block;
  box-sizing: border-box;
  height: 60px;
  width: 25%;
  border-radius: 5px;
  margin-top: 10px;
  border: 0px solid wheat;
  float: left;
  padding: 20px 5px;
  text-align: center;
  font-size: 14px;
  font-weight: 600;
  background-color: #13227a;
  color: #fff;
}
.rightWrap2 {
  display: inline-block;
  box-sizing: border-box;
  height: 60px;
  width: 15%;
  border-radius: 5px;
  margin-top: 10px;
  border: 0px solid wheat;
  float: left;
  padding: 20px 0px;
  text-align: center;
  font-size: 14px;
  font-weight: 600;
  background-color: rgba(255, 0, 0, 0.705);
  color: #fff;
}
.seledOnce_no {
  display: block;
  width: 100%;
  height: 80px;
  text-align: center;
  line-height: 80px;
}